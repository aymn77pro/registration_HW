 <ImageView
     android:id="@+id/imageView"
     android:layout_width="83dp"
     android:layout_height="50dp"
     android:layout_marginEnd="5dp"
     app:layout_constraintBottom_toBottomOf="@+id/name"
     app:layout_constraintEnd_toStartOf="@+id/textView3"
     app:layout_constraintHorizontal_bias="0.0"
     app:layout_constraintStart_toStartOf="parent"
     app:layout_constraintTop_toTopOf="@+id/name"
     app:layout_constraintVertical_bias="1.0"
     app:srcCompat="@drawable/ic_name" />

 <ImageView
     android:id="@+id/imageView2"
     android:layout_width="95dp"
     android:layout_height="58dp"
     android:layout_marginEnd="8dp"
     app:layout_constraintBottom_toBottomOf="@+id/Email"
     app:layout_constraintEnd_toStartOf="@+id/textView4"
     app:layout_constraintTop_toTopOf="@+id/Email"
     app:layout_constraintVertical_bias="0.0"
     app:srcCompat="@drawable/ic_name" />

 <ImageView
     android:id="@+id/imageView3"
     android:layout_width="100dp"
     android:layout_height="56dp"
     app:layout_constraintBottom_toBottomOf="@+id/editTextTextPassword1"
     app:layout_constraintTop_toTopOf="@+id/editTextTextPassword1"
     app:layout_constraintVertical_bias="1.0"
     app:srcCompat="@drawable/ic_name"
     tools:layout_editor_absoluteX="-11dp" />

 <ImageView
     android:id="@+id/imageView4"
     android:layout_width="66dp"
     android:layout_height="53dp"
     android:layout_marginEnd="12dp"
     app:layout_constraintBottom_toBottomOf="@+id/editTextTextPassword2"
     app:layout_constraintEnd_toStartOf="@+id/textView6"
     app:layout_constraintTop_toTopOf="@+id/editTextTextPassword2"
     app:layout_constraintVertical_bias="0.666"
     app:srcCompat="@drawable/ic_name" />

 <ImageView
     android:id="@+id/imageView5"
     android:layout_width="59dp"
     android:layout_height="54dp"
     android:layout_marginEnd="20dp"
     app:layout_constraintBottom_toBottomOf="@+id/birth"
     app:layout_constraintEnd_toStartOf="@+id/textView7"
     app:layout_constraintTop_toTopOf="@+id/birth"
     app:layout_constraintVertical_bias="0.0"
     app:srcCompat="@drawable/ic_name" />

 <ImageView
     android:id="@+id/imageView6"
     android:layout_width="81dp"
     android:layout_height="84dp"
     android:layout_marginEnd="24dp"
     app:layout_constraintBottom_toBottomOf="@+id/radioGroup"
     app:layout_constraintEnd_toStartOf="@+id/textView8"
     app:layout_constraintTop_toTopOf="@+id/radioGroup"
     app:layout_constraintVertical_bias="1.0"
     app:srcCompat="@drawable/ic_baseline_person_24"
     tools:ignore="MissingConstraints" />

